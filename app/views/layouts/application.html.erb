<!DOCTYPE html>
<html>
  <head>
    <title>MembersOnly</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
  </head>

  <body>
        <%= render "layouts/header"%>
      <div class="container">
          <% if user_signed_in?%>
          <% else%>
              
          <% end%>
          <% if notice%>
          <div  class = "alert alert-primary mt-5"><%= notice%> </p> </div>
          <% elsif alert%>
          <div  class = "alert alert-danger mt-5"><%= alert%> </p> </div>
          <%end%>
     

        <div class="grid text-center">
          <div class="g-col-6 g-col-md-4">   <%= yield %></div>
        </div>
      </div>
  </body>
</html>
